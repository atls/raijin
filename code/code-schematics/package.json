{
  "name": "@atls/code-schematics",
  "version": "2.0.2",
  "license": "BSD-3-Clause",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn run build:schematic-factory && yarn run build:library",
    "build:library": "yarn library build",
    "build:schematic-factory": "yarn node ./src/scripts/schematic-factory-build.script.ts",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist"
  },
  "dependencies": {
    "@angular-devkit/core": "19.1.5",
    "@angular-devkit/schematics": "19.1.5",
    "strip-json-comments": "3.1.1"
  },
  "devDependencies": {
    "@atls/code-runtime": "workspace:*",
    "@types/node": "22.13.10",
    "@yarnpkg/cli": "4.5.1",
    "@yarnpkg/core": "4.1.4",
    "@yarnpkg/esbuild-plugin-pnp": "3.0.0-rc.15",
    "@yarnpkg/fslib": "3.1.0",
    "esbuild": "0.24.2",
    "rxjs": "7.8.1"
  },
  "peerDependencies": {
    "@yarnpkg/cli": "*",
    "@yarnpkg/core": "*"
  },
  "publishConfig": {
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "typings": "dist/index.d.ts"
  },
  "typecheckSkipLibCheck": true
}
