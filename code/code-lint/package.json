{
  "name": "@atls/code-lint",
  "version": "2.0.14",
  "license": "BSD-3-Clause",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn library build",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist"
  },
  "dependencies": {
    "@atls/code-runtime": "workspace:*",
    "globby": "13.2.2",
    "ignore": "5.3.2"
  },
  "devDependencies": {
    "@types/node": "22.13.10"
  },
  "peerDependencies": {
    "react": "*",
    "typescript": "*"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts"
  },
  "typecheckSkipLibCheck": true
}
