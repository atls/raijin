{
  "name": "@atls/code-lint",
  "version": "0.0.34",
  "license": "BSD-3-Clause",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn library build",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist"
  },
  "dependencies": {
    "@atls/code-runtime": "workspace:*",
    "deepmerge": "^4.3.1",
    "globby": "^13.2.2",
    "ignore": "^5.2.4"
  },
  "devDependencies": {
    "@types/node": "^20.14.9"
  },
  "peerDependencies": {
    "react": "*",
    "typescript": "*"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts"
  }
}
