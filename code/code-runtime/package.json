{
  "name": "@atls/code-runtime",
  "version": "0.0.0",
  "license": "BSD-3-Clause",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts",
    "./eslint": "./src/eslint.ts",
    "./webpack": "./src/webpack.ts",
    "./jest": "./src/jest.ts",
    "./typescript": "./src/typescript.ts"
  },
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn library build",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist"
  },
  "dependencies": {
    "@atls/config-eslint": "workspace:*",
    "@atls/config-jest": "workspace:*",
    "@atls/webpack-proto-imports-loader": "workspace:*",
    "@atls/webpack-start-server-plugin": "workspace:*",
    "@jest/core": "^29.5.0",
    "eslint": "^8.37.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.3",
    "webpack": "^5.78.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.11"
  },
  "publishConfig": {
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "./eslint": {
        "import": "./dist/eslint.js",
        "default": "./dist/eslint.js",
        "types": "./dist/eslint.d.ts"
      },
      "./webpack": {
        "import": "./dist/webpack.js",
        "default": "./dist/webpack.js",
        "types": "./dist/webpack.d.ts"
      },
      "./jest": {
        "import": "./dist/jest.js",
        "default": "./dist/jest.js",
        "types": "./dist/jest.d.ts"
      },
      "./typescript": {
        "import": "./dist/typescript.js",
        "default": "./dist/typescript.js",
        "types": "./dist/typescript.d.ts"
      }
    },
    "main": "dist/index.js",
    "typings": "dist/index.d.ts"
  }
}
