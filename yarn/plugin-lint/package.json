{
  "name": "@atls/yarn-plugin-lint",
  "version": "1.0.6",
  "private": true,
  "license": "BSD-3-Clause",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./sources/index.ts"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "builder build plugin",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist"
  },
  "dependencies": {
    "@atls/cli-ui-error-info-component": "workspace:*",
    "@atls/cli-ui-lint-progress-component": "workspace:*",
    "@atls/cli-ui-lint-result-component": "workspace:*",
    "@atls/cli-ui-renderer-static-component": "workspace:*",
    "@atls/code-lint": "workspace:*",
    "@atls/yarn-run-utils": "workspace:*",
    "clipanion": "4.0.0-rc.2",
    "ink": "3.2.0",
    "react": "18.3.1"
  },
  "devDependencies": {
    "@atls/yarn-test-utils": "workspace:*",
    "@types/react": "18.3.18",
    "@yarnpkg/builder": "4.2.0",
    "@yarnpkg/cli": "4.5.1",
    "@yarnpkg/core": "4.1.4",
    "@yarnpkg/fslib": "3.1.0",
    "typescript": "5.5.4"
  },
  "peerDependencies": {
    "@yarnpkg/cli": "*",
    "@yarnpkg/core": "*"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts"
  }
}
