{
  "name": "@atls/yarn-plugin-ui",
  "version": "1.0.0",
  "private": true,
  "license": "BSD-3-Clause",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./sources/index.ts"
  },
  "main": "sources/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "builder build plugin",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist"
  },
  "dependencies": {
    "@atls/cli-ui-error-info": "workspace:*",
    "@atls/cli-ui-icons-progress": "workspace:*",
    "@atls/cli-ui-renderer-static": "workspace:*",
    "@atls/code-icons": "workspace:*",
    "clipanion": "4.0.0-rc.4",
    "globby": "13.2.2",
    "ink": "5.0.1",
    "react": "18.3.1"
  },
  "devDependencies": {
    "@atls/yarn-run-utils": "workspace:*",
    "@atls/yarn-test-utils": "workspace:*",
    "@types/react": "18.3.5",
    "@yarnpkg/builder": "4.2.0",
    "@yarnpkg/cli": "4.5.0",
    "@yarnpkg/core": "4.1.3",
    "@yarnpkg/fslib": "3.1.0"
  },
  "peerDependencies": {
    "@yarnpkg/cli": "*",
    "@yarnpkg/core": "*"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts"
  }
}
