{
  "name": "@atls/cli-ui-source-component",
  "version": "1.0.0",
  "license": "BSD-3-Clause",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn library build",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist"
  },
  "dependencies": {
    "@babel/code-frame": "7.24.2",
    "supports-color": "9.4.0"
  },
  "devDependencies": {
    "@types/babel__code-frame": "7",
    "@types/node": "20",
    "@types/react": "16",
    "ink": "3.2.0"
  },
  "peerDependencies": {
    "ink": "*",
    "react": "*"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts"
  }
