{
  "name": "@atlantis-lab/actl-check",
  "version": "0.0.0",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "oclif": {
    "bin": "actl",
    "commands": "./lib/commands",
    "topics": {
      "check": {
        "description": "Github checks"
      }
    }
  },
  "scripts": {
    "prepack": "yarn build",
    "postpack": "rm -f oclif.manifest.json",
    "build": "rm -rf lib && tsc -b tsconfig.json && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md"
  },
  "dependencies": {
    "@actions/core": "1.2.0",
    "@actions/github": "1.1.0",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "2.2.2",
    "@octokit/graphql": "4.3.1",
    "@types/jest": "24.0.23",
    "@monstrs/config": "0.2.2",
    "commitlint-format-json": "1.1.0",
    "eslint": "6.7.2",
    "execa": "3.4.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/eslint": "6.1.3",
    "@types/execa": "2.0.0",
    "@types/node": "14.0.14",
    "typescript": "3.9.6"
  }
}
